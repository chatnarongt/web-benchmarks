{
  "cSpell.words": [
    "Swashbuckle"
  ],
  "editor.defaultFormatter": "csharpier.csharpier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.csharpier": "explicit"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.indentSize": "tabSize",
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.indentSize": "tabSize",
    "editor.tabSize": 2
  }
}